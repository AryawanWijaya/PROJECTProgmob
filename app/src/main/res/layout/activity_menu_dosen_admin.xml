<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuDosenAdminActivity">

    <ImageButton
        android:id="@+id/btnDataDiriDosenAdmin"
        android:layout_width="103dp"
        android:layout_height="90dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="88dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_data_diri" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:text="Daftar Dosen"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/btnDaftarDosenAdmin" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="36dp"
        android:text="Daftar Matkul"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnKelolaDaftarAdmin" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="8dp"
        android:text="Data Diri"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDataDiriDosenAdmin" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="8dp"
        android:text="Kelola KRS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnKelolaKrsAdmin" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:text="Daftar Mahasiswa"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/btnDaftarMhsAdmin" />

    <ImageButton
        android:id="@+id/btnDaftarDosenAdmin"
        android:layout_width="98dp"
        android:layout_height="89dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="16dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/btnKelolaDaftarAdmin"
        app:layout_constraintStart_toEndOf="@+id/btnDataDiriDosenAdmin"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_daftar_dosen" />

    <ImageButton
        android:id="@+id/btnKelolaDaftarAdmin"
        android:layout_width="105dp"
        android:layout_height="89dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="28dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_daftar_matkul" />

    <ImageButton
        android:id="@+id/btnKelolaKrsAdmin"
        android:layout_width="100dp"
        android:layout_height="91dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/logo_kelola_krs" />

    <ImageButton
        android:id="@+id/btnDaftarMhsAdmin"
        android:layout_width="96dp"
        android:layout_height="91dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/btnKelolaKrsAdmin"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/logo_daftar_mhs" />

</androidx.constraintlayout.widget.ConstraintLayout>