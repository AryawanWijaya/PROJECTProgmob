<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuDosenActivity">

    <ImageButton
        android:id="@+id/btnLihatKelas"
        android:layout_width="94dp"
        android:layout_height="88dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="36dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_lihat_kelas" />

    <ImageButton
        android:id="@+id/btnDataDiriDosen"
        android:layout_width="103dp"
        android:layout_height="90dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="88dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_data_diri" />

    <ImageButton
        android:id="@+id/btnLihatKrs"
        android:layout_width="98dp"
        android:layout_height="90dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="10dp"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toStartOf="@+id/btnLihatKelas"
        app:layout_constraintStart_toEndOf="@+id/btnDataDiriDosen"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_daftar_krs" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="12dp"
        android:text="Data Diri"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDataDiriDosen" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="12dp"
        android:text="Lihat KRS"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/btnLihatKrs" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="56dp"
        android:text="Lihat Kelas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLihatKelas" />
</androidx.constraintlayout.widget.ConstraintLayout>